        "            'å¼€å¿ƒ': (0.8, 0.6),   'å…´å¥‹': (0.7, 0.9),\n",
        "            'å¹³é™': (0.4, -0.7),  'æ»¡è¶³': (0.6, -0.4),\n",
        "            'æ‚²ä¼¤': (-0.6, -0.5), 'æ²®ä¸§': (-0.4, -0.6),\n",
